{
 "name":"物品",
 //游戏该物品的名字,
 "description":"这是个物品",
 //物品介绍
 "cost":1,
 //决定建筑建造时间,60:1秒
 "hardness":0,
 //矿石等级,这个数值比钻头挖掘等级低,钻头就可以挖掘
 "lowPriority":true,
 //兵种是否可以挖这个矿,是
 "healthScaling":0,
 //该物品为建筑材料是会增加建筑的实际血量               算法:建筑默认血量×(1+这个值)
 "color":"ffe600",
 //物品分类器等一系列类似的                                建筑选中该物品时显示的颜色                             
 "radioactivity":0,
 //放射性,RTG发电机使用改物品会增加发电量               算法:发电机本身的基础发电量×这个值的100%=实际发电量  
 "flammability":0,
 //燃烧性,火力发电机使用该物品会增加发电量               算法:发电机本身的基础发电量×这个值的100%=实际发电量  
 "charge":0,
 //放电性,该物品在容器内时,容器爆炸会产生电击伤害
 "explosiveness":0,
 //爆炸性,可以提升该物品在容器内时,容器的爆炸伤害
 "buildable":true,
 //能否放进核心,是
 "hidden":false,
 //是否隐藏该物品,否
 "frames":4,
 //动画的贴图数量,0时不会有动画
 "transitionFrames":1,
 //两个帧之间的过渡帧数
 "frameTime":3,
 //帧切换的时间
 "research":"科技",
 //解锁这个物品需要什么科技
}